@prefix pml: <http://provenanceweb.org/ns/pml#> . 
@prefix xpl: <http://niche.cs.dal.ca/xpl#> .

	{?p :hasPhlemAmount :Moderate}
=>
	{?p :isStratified :YellowZone ; xsd:dueTo :hasPhlemAmount} .

	{?p :hasColoredPhlem "true"^^xsd:boolean}
=>
	{?p :isStratified :YellowZone ; xsd:dueTo :hasColoredPhlem} .

	{?p :hasSwollenAnkle "true"^^xsd:boolean}
=>
	{?p :isStratified :YellowZone} .

	{?p :hasActivityExertion :Moderate}
=>
	{?p :isStratified :YellowZone} .

	{?p :hasCyanosis "true"^^xsd:boolean}
=>
	{?p :isStratified :RedZone} .

	{?p :hasChestPain "true"^^xsd:boolean}
=>
	{?p :isStratified :RedZone} .

	{?p :hasActivityExertion :Severe}
=>
	{?p :isStratified :RedZone} .

	{?p :hasCough :Severe}
=>
	{?p :isStratified :RedZone} .

	{?p :hasLow02Saturation "true"^^xsd:boolean}
=>
	{?p :isStratified :RedZone} .

	{?p :hasFever :Severe}
=>
	{?p :isStratified :RedZone} .

	{?p :hasConfusion "true"^^xsd:boolean}
=>
	{?p :isStratified :RedZone} .

	{?p :hasPhlemAmountValue ?v . ?v math:greaterThan 5 ; math:lessThan 8}
=>
	{?p :hasPhlemAmount :Moderate} .

