# - Stratification

# -- Green Zone

{
   ?p :hasPhlegmAmount :Low .
   ?p :hasActivityExertion :Low 
}
=> 
{
   ?p :isStratified :GreenZone 
}
;
   a x:InputRule .

#
{
   ?p :hasPhlegmAmountValue ?v .
   ?v math:notGreaterThan 5 
}
=> 
{
   ?p :hasPhlegmAmount :Low 
}
;
   a x:InputRule .

#
{
   ?p :hasActivityExertionValue ?v .
   ?v math:notGreaterThan 4 
}
=> 
{
   ?p :hasActivityExertion :Low 
}
;
   a x:InputRule .



