#
{
   (
       ?inf
       {
           ?i a x:RootNode ;
               r:gives ?concl .
           (
               ?concl
               {
                   ?a x:padding ?a 
               }
           ) e:graphDifference ?inf .
       }
       ?infs
   ) log:collectAllIn _:t .
   ?infs list:unique ?uniqueInfs .
#    "T0" log:trace ?uniqueInfs .
   ?uniqueInfs list:member ?inf .
   (
       ?ev
       {
           ?i a x:RootNode .
           ?i!r:gives log:includes ?inf .
           ?i r:evidence ?ev .
       }
       ?evs
   ) log:collectAllIn _:t .
   ?evs!list:append list:unique ?combinedEv .
#    "T1" log:trace ?combinedEv .
}
=> 
{
   [ a x:RootNode , x:InferenceNode ;
       r:gives ?inf ;
       r:evidences ?combinedEv 
   ]
} .
