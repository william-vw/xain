#
{
   ?e a r:Extraction .
   ?x!r:evidence list:member ?e .
}
=> 
{
   ?e a x:RootNode , x:NodeOfInterest 
} .


#
{
   ?n a x:NodeOfInterest .
   ?n list:in ?x!r:evidence .
   _:t log:notIncludes 
   {
       ?y r:component ?x 
   } .
}
=> 
{
   ?x a x:NodeOfInterest 
} .


#
{
   ?n x:support ?support 
}
<= 
{
   (
       ?x
       {
           ?n list:in ?x 
       }
       ?support
   ) log:collectAllIn _:t .
} .
